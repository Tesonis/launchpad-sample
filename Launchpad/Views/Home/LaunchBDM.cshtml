        //Create and Update uses this function
        function SubmitLaunch() {
            $("#section_error").html("");
            $("#btn-CompleteApprove").addClass('disabled');
            var launchtype = $("#select_launchtype").val();
            var itempayloads = [];
            var kacpayloads = [];
            var itemrows = $("#table_launchitems tbody tr");
            for (i = 0; i < itemrows.length; i++) {
                var itemid = itemrows.eq(i).find(".sorting_1").text();
                var sampleavail = itemrows.eq(i).find(".data-sampleavail").val();
                var launchcomment = itemrows.eq(i).find(".data-comment").text();
                var item = { Itemid: itemid, Sampleavail: sampleavail, Comment: launchcomment };
                itempayloads.push(item);
            }
            $("#table_kac").DataTable().destroy();
            var kacrows = $("#table_kac tbody tr");
            for (i = 0; i < kacrows.length; i++) {
                var customercode = kacrows.eq(i).find("td").eq(0).text();
                var initstatus = kacrows.eq(i).find("select").val();
                var customeraccount = { Customercode: customercode, Status: initstatus }
                kacpayloads.push(customeraccount);
            }
            $.ajax({
                type: "POST",
                url: '@Url.Action("SubmitLaunch", "Home")',
                dataType: 'JSON',
                data: { launchtype: launchtype, items: JSON.stringify(itempayloads), customeracc: JSON.stringify(kacpayloads)},
                success: function (response) {
                    if (response.status == "error") {
                        $("#section_error").html(RenderError(response.errorcode, response.errormsg.toString(), response.errorsrc.toString()));
                        $("#btn-CompleteApprove").removeClass('disabled');
                    }
                    else {
                        window.location.replace(response.url);
                    }
                },
                error: function (request, status, error) {
                    alert('Error occured');
                    alert(request.responseText);
                }
            });
        }
        function SubmitUpdate() {
            $("#section_error").html("");
            $("#btn-CompleteApprove").addClass('disabled');
            var updatepayloads = [];
            var itemrows = $("#table_updatestatus tbody tr");
            for (i = 0; i < itemrows.length; i++) {
                var launchid = itemrows.eq(i).find(".data-launchid").text();
                var launchtype = itemrows.eq(i).find(".data-launchtype").text();
                var sampleavail = itemrows.eq(i).find(".data-sampleavail").text();
                var comment = itemrows.eq(i).find(".data-comment").text();
                var status = itemrows.eq(i).find("select").val();
                var launch = { Launchid: launchid, Launchtype: launchtype, Sampleavail: sampleavail, Comment: comment, Status: status };
                updatepayloads.push(launch);
            }
            $.ajax({
                type: "POST",
                url: '@Url.Action("SubmitUpdateBDM", "Home")',
                dataType: 'JSON',
                data: { updatejson: JSON.stringify(updatepayloads)},
                success: function (response) {
                    if (response.status == "error") {
                        $("#section_error").html(RenderError(response.errorcode, response.errormsg.toString(), response.errorsrc.toString()));
                        $("#btn-CompleteApprove").removeClass('disabled');
                    }
                    else {
                        window.location.replace(response.url);
                    }
                },
                error: function (request, status, error) {
                    alert('Error occured');
                    alert(request.responseText);
                }
            });
        }
